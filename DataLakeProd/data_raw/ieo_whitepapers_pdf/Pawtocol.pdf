  <style>
    .preload * {
      -webkit-transition: none !important;
      -moz-transition: none !important;
      -ms-transition: none !important;
      -o-transition: none !important;
      transition: none !important
    }
    #sidebar {
      transition: all .3s cubic-bezier(1, .5, .8, 1)
    }
    .slide-fade-enter-active,
    .slide-fade-leave-active {
      transition: all .3s cubic-bezier(1, .5, .8, 1)
    }
    .slide-fade-enter,
    .slide-fade-leave-to {
      transform: translateX(-300px)
    }
    .flex-crunch-enter-active,
    .flex-crunch-leave-active {
      transition: all .3s
    }
    .flex-crunch-enter,
    .flex-crunch-leave-to {
      width: 0
    }
    .progress-bar {
      height: 30px;
      width: 100%;
      border-bottom: 1px solid #f0f0f0;
      display: flex;
      justify-content: center;
      align-items: center;
      position: relative;
      overflow: hidden;
      border-radius: 3px 3px 0 0
    }
    .text {
      font-size: 14px;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate3d(-50%, -50%, 0);
      z-index: 99
    }
    .filled-bar {
      position: absolute;
      top: 0;
      left: 0;
      height: 100%;
      width: 100%;
      transform: translate3d(-100%, 0, 0);
      background: #eef0ff;
      z-index: 99
    }
    .alert-toast {
      -webkit-animation: slide-in-right .5s cubic-bezier(.25, .46, .45, .94) both;
      animation: slide-in-right .5s cubic-bezier(.25, .46, .45, .94) both
    }
    .alert-toast input:checked~* {
      -webkit-animation: fade-out-right .7s cubic-bezier(.25, .46, .45, .94) both;
      animation: fade-out-right .7s cubic-bezier(.25, .46, .45, .94) both
    }
    @-webkit-keyframes slide-in-right {
      0% {
        -webkit-transform: translateX(1000px);
        transform: translateX(1000px);
        opacity: 0
      }
      100% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
        opacity: 1
      }
    }
    @keyframes slide-in-right {
      0% {
        -webkit-transform: translateX(1000px);
        transform: translateX(1000px);
        opacity: 0
      }
      100% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
        opacity: 1
      }
    }
    @-webkit-keyframes fade-out-right {
      0% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
        opacity: 1
      }
      100% {
        -webkit-transform: translateX(50px);
        transform: translateX(50px);
        opacity: 0
      }
    }
    @keyframes fade-out-right {
      0% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
        opacity: 1
      }
      100% {
        -webkit-transform: translateX(50px);
        transform: translateX(50px);
        opacity: 0
      }
    }
