<script>

    $(function() {

        var cvs = document.getElementById("white");
        var w = $('.right').width();
        var h = w*1.415;
        var p = 0;
        cvs.width = w*2;
        cvs.height = h*2;

        var white_paper_lang = $('.white-paper').attr('data-l');
        if(white_paper_lang == 'kr'){
            loadImage("/images/z/2/0.jpg");
        }
        else {
            loadImage("/images/z/1/0.jpg");
        }


        function loadImage(_path)
        {
            var imgObj = new Image();
            imgObj.src = _path;

            imgObj.onload = function(){
                var ctx = cvs.getContext('2d');
                ctx.clearRect(0, 0, w*2, h*2);
                ctx.drawImage(this, 0, 0, w*2, h*2);
            };
        }



        // li
        $('section.white-paper .left li').click(function () {
            var data = {
                page:$(this).attr('data-id'),
                lang:white_paper_lang
            };
            ax('/api/white-paper','post', data, whitepaperReturn);
        });


        // last button
        $('section.white-paper .last button').click(function () {
            if(p == 0) {
                return false;
            }
            else {
                var data = {
                    page:p-1,
                    lang:white_paper_lang
                };
                ax('/api/white-paper','post', data, whitepaperReturn);
            }
        });

        // next button
        $('section.white-paper .next button').click(function () {
            if(p == ($('.white-paper').attr('data-p') - 1) ) {
                return false;
            }
            else {
                var data = {
                    page:p+1,
                    lang:white_paper_lang
                };
                ax('/api/white-paper','post', data, whitepaperReturn);
            }
        });


        // return
        function whitepaperReturn(e)
        {
            p = parseInt(e.page);
            loadImage(e.img);
            $('section.white-paper .now b').text(p+1);
        }


        // mouse disable
        $('#white').bind("contextmenu",function(e){
            return false;
        });

    });



</script>






